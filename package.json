{
  "name": "pyodide-express-server",
  "version": "1.0.0",
  "description": "A Node.js Express server that executes Python code using Pyodide with REST API, file uploads, and package management",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --config nodemon.json",
    "dev:verbose": "nodemon --config nodemon.json --verbose",
    "dev:inspect": "nodemon --config nodemon.json --inspect",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk",
    "test": "node test-client.js",
    "test:manual": "node test-client.js",
    "test:health": "curl http://localhost:3000/health",
    "lint": "eslint src/**/*.js --fix",
    "format": "prettier --write . || true",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "clean:logs": "rm -rf logs/*.log",
    "clean:uploads": "rm -rf uploads/* && touch uploads/.gitkeep",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:audit": "npm audit",
    "deps:fix": "npm audit fix",
    "build": "echo 'No build step required for this project'",
    "prestart": "node -e \"console.log('Starting Pyodide Express Server...')\"",
    "postinstall": "node -e \"console.log('Dependencies installed successfully!')\"",
    "setup": "npm install && mkdir -p uploads logs && touch uploads/.gitkeep logs/.gitkeep",
    "docker:build": "docker build -t pyodide-express-server .",
    "docker:run": "docker run -p 3000:3000 pyodide-express-server",
    "docker:compose": "docker-compose up --build",
    "docker:compose:down": "docker-compose down",
    "docker:push": "docker push pyodide-express-server:latest",
    "pm2:start": "pm2 start ecosystem.config.json",
    "pm2:stop": "pm2 stop ecosystem.config.json",
    "pm2:restart": "pm2 restart ecosystem.config.json",
    "pm2:reload": "pm2 reload ecosystem.config.json",
    "pm2:delete": "pm2 delete ecosystem.config.json",
    "pm2:logs": "pm2 logs pyodide-express-server",
    "pm2:monit": "pm2 monit",
    "prod:start": "NODE_ENV=production pm2 start ecosystem.config.json --env production",
    "prod:deploy": "npm ci --production && npm run prod:start",
    "heroku-postbuild": "echo 'No build step required'",
    "serve": "npm start"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.21.1",
    "helmet": "^8.1.0",
    "multer": "^2.0.0",
    "pm2": "^6.0.10",
    "pyodide": "^0.28.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-plugin-jsdoc": "^54.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.6.2",
    "supertest": "^7.1.3"
  },
  "optionalDependencies": {
    "form-data": "^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "keywords": [
    "pyodide",
    "python",
    "express",
    "nodejs",
    "api",
    "rest",
    "data-science",
    "webassembly",
    "wasm",
    "pandas",
    "numpy",
    "jupyter",
    "notebook",
    "machine-learning",
    "data-analysis",
    "scientific-computing",
    "server",
    "backend",
    "microservice",
    "swagger",
    "openapi",
    "documentation"
  ],
  "author": {
    "name": "Reinaldo Penno",
    "email": "your.email@example.com",
    "url": "https://github.com/brisacoder"
  },
  "contributors": [],
  "maintainers": [
    {
      "name": "Your Name",
      "email": "your.email@example.com"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/brisacoder/pyodide-express-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brisacoder/pyodide-express-server.git"
  },
  "bugs": {
    "url": "https://github.com/brisacoder/pyodide-express-server/issues",
    "email": "your.email@example.com"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/brisacoder"
  },
  "files": [
    "src/",
    "public/",
    "docs/",
    "examples/",
    "test-client.js",
    "README.md",
    "LICENSE",
    "CONTRIBUTING.md",
    "CHANGELOG.md"
  ],
  "directories": {
    "lib": "./src",
    "test": "./tests",
    "doc": "./docs",
    "example": "./examples"
  },
  "bin": {},
  "man": [],
  "config": {
    "port": 3000,
    "pyodide_version": "0.28.0"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules",
      "uploads",
      "logs",
      "*.log"
    ],
    "delay": 1000,
    "env": {
      "NODE_ENV": "development"
    }
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "commonjs"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn",
      "no-undef": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/?(*.)+(spec|test).js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "browserslist": [
    "node >= 16"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "workspaces": [],
  "overrides": {
    "glob": "^10.3.10",
    "inflight": "npm:@isaacs/inflight@^1.0.6",
    "rimraf": "^5.0.5"
  },
  "resolutions": {
    "glob": "^10.3.10",
    "inflight": "npm:@isaacs/inflight@^1.0.6",
    "rimraf": "^5.0.5"
  },
  "packageManager": "npm@9.8.1"
}
