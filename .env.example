# Server Configuration
PORT=3000
NODE_ENV=development

# Pyodide Configuration
PYODIDE_INDEX_URL=https://cdn.jsdelivr.net/pyodide/v0.28.0/full/
PYODIDE_PACKAGES=numpy,pandas,matplotlib,scipy

# Security
API_KEY=your_api_key_here
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=15

# File Uploads
MAX_FILE_SIZE=10485760
UPLOAD_DIR=uploads

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log